{
  "name": "graphql-ts-jwt-auth",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "prepare": "husky install",
    "pre-commit": "lint-staged",
    "codegen": "graphql-codegen --config codegen.yml"
